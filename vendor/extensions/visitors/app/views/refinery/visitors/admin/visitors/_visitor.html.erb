<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(visitor) -%>">
  <span class='title'>
    <%= visitor.parking %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.visitors_visitor_path(visitor),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_visitors_admin_visitor_path(visitor),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.visitors_admin_visitor_path(visitor),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => visitor.parking),
        :method => :delete %>
  </span>
</li>
