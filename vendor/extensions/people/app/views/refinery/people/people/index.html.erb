<div class='pure-g-r'>
	<div class='pure-u-1-5'>
	</div>
	<div class='pure-u-2-3'>
	<h1>People</h1>
	<h1>Directors</h1>
	<div class='pure-g-r'>
  <% @directors.each do |person| %>
  		<div class='pure-u-1-5'>
	     	<div><%= image_fu person.photo, '100x' %></div>
	      <div><%= person.name %></div>
	      <div><%= person.title %></div>
	      <div><%= person.email %></div>
	      <div><%= person.phone %></div>
      </div>
  <% end %>
  </div>
  <h1>Associates</h1>
  <div class='pure-g-r'>
  <% @associates.each do |person| %>
    <div class='pure-u-1-5'>
	     	<div><%= image_fu person.photo, '100x' %></div>
