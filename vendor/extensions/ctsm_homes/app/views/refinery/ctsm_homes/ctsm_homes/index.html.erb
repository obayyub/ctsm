<div class="row">

<div class="col-lg-9" ng-controller='ArticlesCtrl'>
		<h1>CTSM News</h1>
		<div ng-repeat='article in articles'>
			<div class='ctsmnews'>
				<a href='{{article.link}}'>{{article.title}}</a>
			</div>
			<div ng-bind-html-unsafe="article.content">
			</div>
			<div class="share-buttons">
				<a href="https://twitter.com/share" class="twitter-share-button" data-url="http://ctsmtest.com" data-text="ctsmtesttitle" data-via="marcus_orochena">Tweet</a>
				<%= render 'refinery/ctsm_homes/ctsm_homes/tweetscript' %>
			</div>
			<hr>
		</div>
	</div>

<div class="col-lg-3 ctsm-engineering-news" ng-controller='FeedCtrl'>
	<h4>Engineering News</h4>
	<div ng-repeat='feed in feeds'>
		<a href='{{feed.link}}'>{{feed.title}}</a><hr>
	</div>
</div>
</div>
